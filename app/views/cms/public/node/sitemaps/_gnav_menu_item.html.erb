<%-
  return '' unless gnav_menu_item.sitemap_visible?
  css_class = "#{css_class + '-' if css_class.present?}#{File.basename(gnav_menu_item.name, '.*').camelize(:lower)}"
-%>
<li class="level<%= level %> <%= css_class %>">
  <%= link_to gnav_menu_item.title, gnav_menu_item.public_uri, class: "level#{level}" %>
</li>
